{
    "name" : "allofmex/translating-autoloader",
    "require" : {
        "symfony/yaml" : "^5"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~9",
        "composer/composer" : "^1.0"
    },
    "autoload" : {
        "psr-4" : {
            "Allofmex\\TranslatingAutoLoader\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Allofmex\\TranslatingAutoLoader\\" : "tests/",
            "Allofmex\\TranslatingAutoLoader\\Test\\" : "tests/assets"
        }
    }
}